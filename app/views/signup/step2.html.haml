お届け先住所入力
= form_for @user, url: signup_index_path, method: :post do |f|
  .single-container__single-main__account-single-container__registration-form__upper-box
    .single-container__single-main__account-single-container__registration-form__upper-box__input-box
      = f.label "郵便番号"
      %span.single-container__single-main__account-single-container__registration-form__upper-box__input-box__importance
        必須
      = f.text_field :current_address_postal_code, placeholder: "例) 123-4567", class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__text"
    .single-container__single-main__account-single-container__registration-form__upper-box__input-box
      = f.label "都道府県"
      %span.single-container__single-main__account-single-container__registration-form__upper-box__input-box__importance
        必須
      .single-container__single-main__account-single-container__registration-form__upper-box__input-box__select-box
        = fa_icon "chevron-down" ,class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__select-box__icon"
        = f.select :current_address_prefectures,[["---",""],["北海道","北海道"],["青森県","青森県"],["岩手県","岩手県"],["宮城県","宮城県"],["秋田県","秋田県"],["山形県","山形県"],["福島県","福島県"],["茨城県","茨城県"],["栃木県","栃木県"],["群馬県","群馬県"],["埼玉県","埼玉県"],["千葉県","千葉県"],["東京都","東京都"],["神奈川県","神奈川県"],["新潟県","新潟県"],["富山県","富山県"],["石川県","石川県"],["福井県","福井県"],["山梨県","山梨県"],["長野県","長野県"],["岐阜県","岐阜県"],["静岡県","静岡県"],["愛知県","愛知県"],["三重県","三重県"],["滋賀県","滋賀県"],["京都府","京都府"],["大阪府","大阪府"],["兵庫県","兵庫県"],["奈良県","奈良県"],["和歌山県","和歌山県"],["鳥取県","鳥取県"],["島根県","島根県"],["岡山県","岡山県"],["広島県","広島県"],["山口県","山口県"],["徳島県","徳島県"],["香川県","香川県"],["愛媛県","愛媛県"],["高知県","高知県"],["福岡県","福岡県"],["佐賀県","佐賀県"],["長崎県","長崎県"],["熊本県","熊本県"],["大分県","大分県"],["宮崎県","宮崎県"],["鹿児島県","鹿児島県"],["沖縄県","沖縄県"]], class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__select-box__select-lists"
    .single-container__single-main__account-single-container__registration-form__upper-box__input-box
      = f.label "市区町村"
      %span.single-container__single-main__account-single-container__registration-form__upper-box__input-box__importance
        必須
      = f.text_field :current_address_city, placeholder: "例) 横浜市緑区 ", class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__text"
    .single-container__single-main__account-single-container__registration-form__upper-box__input-box
      = f.label "番地"
      %span.single-container__single-main__account-single-container__registration-form__upper-box__input-box__importance
        必須
      = f.text_field :current_address, placeholder: "例) 青山1-1-1", class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__text"
    .single-container__single-main__account-single-container__registration-form__upper-box__input-box
      = f.label "建物名"
      %span.single-container__single-main__account-single-container__registration-form__upper-box__input-box__importance-low
        任意
      = f.text_field :current_address_building, placeholder: "例) 柳ビル103", class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__text"
    .single-container__single-main__account-single-container__registration-form__upper-box__input-box
      = f.label "電話番号"
      %span.single-container__single-main__account-single-container__registration-form__upper-box__input-box__importance-low
        任意
      = f.text_field :tel_number, placeholder: "例) 09012345678", class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__text"
    = f.submit '登録する', class: "single-container__single-main__account-single-container__registration-form__upper-box__input-box__btn"


